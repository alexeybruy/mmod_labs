<Window x:Class="LAB4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LAB4"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="950">
    <Grid x:Name="mainGrid" Background="#FF4C95FF">
        <Grid x:Name="graphGrid3" Margin="0,57,0,0" Background="White">
            <Canvas x:Name="graphGrid"/>
        </Grid>
        <Button x:Name="makeStepButton" Content="Make Step Button" Margin="0,10,30,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" Foreground="Black" Height="29" Click="makeStepButton_Click" HorizontalAlignment="Right" Width="143"/>

    </Grid>
</Window>
